{
  "name": "custom-d/eloquent-async-keys",
  "description": "A eloquent async keys",
  "license": "MIT",
  "keywords": [
    "eloquent",
    "async",
    "keys"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "craig.smith@customd.com"
    }
  ],
  "require": {
    "php": ">=7.2",
	"illuminate/support": "~5.8.0|~6.0",
	"ext-openssl": "*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "CustomD\\EloquentAsyncKeys\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CustomD\\EloquentAsyncKeys\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomD\\EloquentAsyncKeys\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
